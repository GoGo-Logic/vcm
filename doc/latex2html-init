$INFO = 0;
$NO_NAVIGATION = 1;
$CUSTOM_TITLES = 1;
$CHILDLINE = " ";

# http://www.mail-archive.com/latex2html@mcs.anl.gov/msg00872.html
sub custom_title_hook {
	if (!( $mypagename eq $myPageNameBackup )) {
		$myPageNameBackup = $mypagename;
		return $myPageNameBackup;
	};

	return "";
}

sub custom_address {
	return "";
}
